import basic;

table ProposeCount {
    address: Byte20,
    count: Byte4, //le_u32
}

vector ProposeCounts <ProposeCount>;

table CheckpointLockCellData {
    version: byte,
    epoch: Byte8,
    period: Byte4,      //le_u32
    total_period: Byte4, //le_u32
    state_root: Byte32,
    latest_block_hash: Byte32,
    xudt_type_id: Byte32,
    timestamp: Byte32,  // last block height
    propose_count: ProposeCounts,
}

table CheckpointLockWitnessLock {
	proposal: Bytes,
	proof:    Bytes,
}